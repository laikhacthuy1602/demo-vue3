{
  "name": "vue-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue,.jsx,.ts,.tsx --ignore-path .gitignore --fix src",
    "format": "prettier .  --write"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "npm run format",
      "git add ."
    ]
  },
  "dependencies": {
    "eslint-config-prettier": "^8.5.0",
    "pinia": "^2.0.17",
    "vue": "^3.2.37",
    "vue-router": "4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.0",
    "eslint": "^8.20.0",
    "eslint-plugin-vue": "^9.3.0",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vite-plugin-eslint": "^1.7.0",
    "vue-tsc": "^0.38.4"
  }
}
